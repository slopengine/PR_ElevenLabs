<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="10.0" ExtensionBundleId="com.slopengine.elevenlabs" ExtensionBundleVersion="1.0.0" ExtensionBundleName="ElevenLabs Voiceover">
  <ExtensionList>
    <Extension Id="com.slopengine.elevenlabs.panel" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PPRO" Version="[13.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="10.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.slopengine.elevenlabs.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>ElevenLabs Voiceover</Menu>
          <Geometry>
            <Size><Height>600</Height><Width>320</Width></Size>
            <MinSize><Height>400</Height><Width>280</Width></MinSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./client/icons/icon.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
