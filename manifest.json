{
  "manifestVersion": 5,
  "id": "com.slopengine.prelevenlabs",
  "name": "ElevenLabs Voiceover",
  "version": "1.1.0",
  "main": "index.html",
  "host": {
    "app": "premierepro",
    "minVersion": "25.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "elevenLabsPanel",
      "label": {
        "default": "ElevenLabs Voiceover"
      },
      "minimumSize": {
        "width": 280,
        "height": 400
      },
      "preferredDockedSize": {
        "width": 320,
        "height": 600
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon.png",
      "scale": [1, 2]
    }
  ],
  "requiredPermissions": {
    "network": {
      "domains": [
        "https://api.elevenlabs.io"
      ]
    },
    "localFileSystem": "fullAccess",
    "localStorage": "readWrite"
  }
}
